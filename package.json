{
  "name": "browser-manager",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "release": "powershell -ExecutionPolicy Bypass -File ./scripts/build-release.ps1",
    "release:version": "powershell -ExecutionPolicy Bypass -File ./scripts/build-release.ps1 -Version",
    "release:github": "powershell -ExecutionPolicy Bypass -File ./scripts/release-to-github.ps1"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-autostart": "^2.5.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-opener": "^2",
    "element-plus": "^2.13.1",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.13",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.4",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.0.0",
    "@tauri-apps/cli": "^2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.23",
    "happy-dom": "^12.10.3",
    "postcss": "^8.5.6",
    "sass": "^1.97.3",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vitest": "^1.2.0",
    "vue-tsc": "^2.1.10"
  }
}
